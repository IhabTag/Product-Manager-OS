# -------------------------------------------------
# Flowise
# -------------------------------------------------
FLOWISE_PORT=3013
FLOWISE_USERNAME=change_me_flowise
FLOWISE_PASSWORD=change_me_flowise_password
FLOWISE_CORS_ORIGINS=*
FLOWISE_IFRAME_ORIGINS=*
FLOWISE_FILE_SIZE_LIMIT=500mb
FLOWISE_DEBUG=true
FLOWISE_DISABLE_TELEMETRY=true

# -------------------------------------------------
# n8n
# -------------------------------------------------
N8N_PORT=5678
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=change_me_n8n
N8N_BASIC_AUTH_PASSWORD=change_me_n8n_password
N8N_WEBHOOK_URL=https://your-n8n-domain.com/
GENERIC_TIMEZONE=Africa/Cairo
N8N_METRICS=true

# -------------------------------------------------
# NocoDB / root-db Postgres
# -------------------------------------------------
ROOT_DB_HOST=root-db
ROOT_DB_PORT=5432
ROOT_DB_NAME=root_db
ROOT_DB_USER=root_user
ROOT_DB_PASSWORD=change_me_root_db_password

# -------------------------------------------------
# Metabase / metabase-db Postgres
# -------------------------------------------------
METABASE_DB_PORT=25532
MB_DB_HOST=metabase-db
MB_DB_PORT=5432
MB_DB_NAME=metabase
MB_DB_USER=metabase_user
MB_DB_PASS=change_me_metabase_db_password

# -------------------------------------------------
# Qdrant
# -------------------------------------------------
QDRANT_API_KEY=change_me_qdrant_api_key
